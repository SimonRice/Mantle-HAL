OTHER_LDFLAGS = -ObjC
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Mantle-HAL_Tests
PODS_ROOT = ${SRCROOT}
SKIP_INSTALL = YES